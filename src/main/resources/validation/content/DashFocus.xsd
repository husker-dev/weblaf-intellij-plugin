<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:java.awt="java.awt"
           elementFormDefault="qualified">

    <xs:include schemaLocation="AbstractContent.xsd" />

    <!-- com.alee.painter.decoration.content.DashFocus -->
    <xs:complexType name="DashFocus">
        <xs:complexContent>
            <xs:extension base="AbstractContent">

                <!-- Focus rounding -->
                <xs:attribute name="round" type="xs:integer" />

                <!-- Focus stroke -->
                <xs:attribute name="stroke" type="java.awt:Stroke" />

                <!-- Focus color -->
                <xs:attribute name="color" type="java.awt:Color" />

            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

</xs:schema>